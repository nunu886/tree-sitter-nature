{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nature",
      "camelcase": "Nature",
      "title": "Nature",
      "scope": "source.nature",
      "file-types": ["n"],
      "injection-regex": "^nature$",
      "class-name": "TreeSitterNature",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Nature grammar for tree-sitter",
    "authors": [
      {
        "name": "nunu886"
      }
    ],
    "links": {
      "repository": "https://github.com/nunu886/tree-sitter-nature"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
